@model Bio
    <!-- Header Area Start -->
    <header class="top">
        <div class="header-top">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-sm-8 col-xs-12">
                        <div class="header-top-left">
                            <p>HAVE ANY QUESTION ?  @Model.Phone</p>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-12">
                        <div class="header-top-right text-right">
                            <ul>
                                <li><a href="login.html">login</a></li>
                                <li><a href="signup.html">signup</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="header-area two header-sticky">
            <div class="container">
                <div class="row">
                    <div class="col-md-3 col-sm-3 col-xs-6">
                        <div class="logo">
                            <a href="index.html"><img src="~/assets/img/logo/@Model.BigLogo" alt="eduhome" /></a>
                        </div>
                    </div>
                    <div class="col-md-9 col-sm-9 col-xs-6">
                        <div class="content-wrapper text-right">
                            <!-- Main Menu Start -->
                            <div class="main-menu">
                                <nav>
                                    <ul>
                                        <li>
                                            <a asp-controller="Home" asp-action="Index">Home</a>
                                        </li>
                                        <li>
                                            <a asp-controller="About" asp-action="Index">About</a>
                                        </li>
                                        <li>
                                            <a asp-controller="Course" asp-action="Index">Courses</a>
                                        </li>
                                        <li>
                                            <a asp-controller="Event" asp-action="Index">Event</a>
                                        </li>
                                        <li class="hidden-sm">
                                            <a asp-controller="Teacher" asp-action="Index">Teacher</a>
                                        </li>
                                        <li>
                                            <a asp-controller="Blog" asp-action="Index">Blog</a>
                                        </li>
                                        <li><a asp-controller="Contact" asp-action="Index">Contact</a></li>
                                    </ul>
                                </nav>
                            </div>
                            <!--Search Form Start-->
                            <div class="search-btn">
                                <ul data-toggle="dropdown" class="header-search search-toggle">
                                    <li class="search-menu">
                                        <i class="fa fa-search"></i>
                                    </li>
                                </ul>
                                <div class="search">
                                    <div class="search-form">
                                        <form id="search-form" method="post">
                                            <input type="search" id="search-input" placeholder="Search here..." name="search" />
                                            <input type="hidden" name="hidden" value="" />
                                            <span id="search-button">
                                                <span class="icon"><i class="fa fa-search"></i></span>
                                            </span>
                                            <ul id="search-list" style="color:whitesmoke !important">
                                                
                                            </ul>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <!--End of Search Form-->
                            <!-- Main Menu End -->
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="mobile-menu hidden-lg hidden-md hidden-sm"></div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <!-- Header Area End -->
